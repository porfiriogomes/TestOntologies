{
  "@context": {
    "LGeoSIMOntology": "http://www.semanticweb.org/bartirarocha/ontologies/2019/4/LGeoSIMOntology#",
    "id": "@id",
    "type": "@type",
 
    "Person": "LGeoSIMOntology:Person",
    "NeighborHood": "LGeoSIMOntology:NeighborHood",

    "nexTo": {
      "@id": "LGeoSIMOntology:nexToPerson",
      "@container": "@list"
    },
    "livingIn": {
      "@id": "LGeoSIMOntology:livingIn",
      "@type": "@id"
    },
    "locatedAt": {
      "@id": "LGeoSIMOntology:locatedAt",
      "@type": "@id"
    },
    "hasParents": {
      "@id": "LGeoSIMOntology:hasParents",
      "@container": "@list"
    },
    "hasChildren": {
      "@id": "LGeoSIMOntology:hasChildren",
      "@container": "@list"
    }
  }    
}

